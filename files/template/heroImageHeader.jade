include ../config
block info
block color
block content
	if HeaderImage
		tr
			td(align="center" bgcolor="#{bgcolor}" style="padding: 0 0 10px 0;")
				br
				br
				a(href="#{linkCTA}#{prefixTraking}#{data}#{lang}_#{trakingSource}#{sufixTraking}" target="_blank")
					img(src="#{imgLink}#{HeaderImage}" border="0" alt="#{heroImageheader}" hspace="0" vspace="0" style="max-width:100%;")
